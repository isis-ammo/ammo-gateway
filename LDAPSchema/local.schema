# string used to define rank
attributetype ( 1.3.6.1.4.1.6863.2.3.109 NAME 'x-Rank'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15{200}
  SINGLE-VALUE )

# string used to define callsign
attributetype ( 1.3.6.1.4.1.6863.2.3.110 NAME 'x-Callsign'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  ORDERING caseIgnoreOrderingMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15{200}
  SINGLE-VALUE )

# string used to define unit
attributetype ( 1.3.6.1.4.1.6863.2.3.111 NAME 'x-Unit'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  ORDERING caseIgnoreOrderingMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15{300}
  SINGLE-VALUE )

attributetype ( 1.3.6.1.4.1.6863.2.3.112 NAME 'x-Insignia'
  DESC 'Insignia of the unit that the object is associated with'
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.40
  SINGLE-VALUE )



# objectclass used with people address book to define required information
# for State Law 5.7.3 - data is defined by law to be private - limited access allowed
objectclass ( 1.3.6.1.4.1.6863.2.4.57
  NAME 'x-Military'
  DESC 'Adds Military specific Contact information'
  AUXILIARY 
  MAY ( x-Rank $ x-Callsign $ x-Unit $ x-Insignia )
  )
