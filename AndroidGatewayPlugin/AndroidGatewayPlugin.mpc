project(AndroidGatewayPlugin) : aceexe, protobuf {
  after += LibGatewayConnector

  exename=AndroidGatewayPlugin
  exeout=../build/bin
  
  includes += ../LibGatewayConnector/
  libs += gatewayconnector
  libpaths += ../build/lib/
  
  Protobuf_Files {
    protocol/AmmmoMessages.proto
  }
  
  Source_Files {
    main.cpp
    AndroidServiceHandler.cpp
  }
}
